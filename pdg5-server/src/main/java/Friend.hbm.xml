<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "persistent.Friend" table = "friend">
      
      <meta attribute = "class-description">
         This class contains the friend detail. 
      </meta>
      
      <id name = "ID" type = "int" column = "ID">
         <generator class="native"/>
      </id>
      
      <property name = "fromUser" column = "fromUser" type = "int"/>
      <property name = "toUser" column = "toUser" type = "int"/>
      <property name = "lastModified" column = "last_mod" type = "timestamp"/>
      
   </class>
</hibernate-mapping>